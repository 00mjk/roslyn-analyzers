<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard1.3</TargetFramework>
    <IsPackable>true</IsPackable>
    <IncludeBuildOutput>false</IncludeBuildOutput>
    <NuspecPackageId>PerformanceSensitive.Analyzers</NuspecPackageId>
    <Description>PerformanceSensitive Analyzers</Description>
    <Summary>PerformanceSensitive Analyzers</Summary>
    <ReleaseNotes>PerformanceSensitive Analyzers</ReleaseNotes>
    <PackageTags>Roslyn CodeAnalysis Compiler CSharp VB VisualBasic Diagnostic Analyzers Syntax Semantics Performance</PackageTags>
    <ContainsPortedFxCopRules>true</ContainsPortedFxCopRules>
  </PropertyGroup>

  <ItemGroup>
    <AnalyzerNupkgAssembly Include="PerformanceSensitive.Analyzers.dll" />
    <AnalyzerNupkgAssembly Include="PerformanceSensitive.CSharp.Analyzers.dll" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\src\PerformanceSensitive.Analyzers\Core\PerformanceSensitive.Analyzers.csproj" />
    <ProjectReference Include="..\..\src\PerformanceSensitive.Analyzers\CSharp\PerformanceSensitive.CSharp.Analyzers.csproj" />
  </ItemGroup>
</Project>